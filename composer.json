{
    "name": "drmer/laravel-enhancer",
    "type": "library",
    "description": "This library collects some useful or funny features but refused to be added to the core.",
    "keywords": [
        "enhancer",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Donald",
            "email": "donald@drmer.net"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Drmer\\Laravel\\Enhancer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drmer\\Tests\\Laravel\\Enhancer\\": "tests"
        }
    }
}
